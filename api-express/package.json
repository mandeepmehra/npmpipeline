{
  "name": "api-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development npm-run-all server",
    "test": "mocha --require @babel/register './tests/**/*.spec.js' --recursive ",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist --source-maps",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "debug": "node --inspect-brk ./dist/bin/www",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "mongoose": "^5.5.9",
    "nodemon": "^1.19.0",
    "supertest": "^4.0.2"
  }
}
